{
  "Nmap": ["nmap -sC -sV -oN host.txt 10.0.0.0/24", "nmap -p- -T4 target.com"],
  "Masscan": ["masscan -p1-65535 10.0.0.0/24 --rate=1000"],
  "Rustscan": ["rustscan -a target.com -- -A"],
  "Amass": ["amass enum -d target.com"],
  "Subfinder": ["subfinder -d target.com"],
  "Assetfinder": ["assetfinder target.com"],
  "WhatWeb": ["whatweb target.com"],
  "Wappalyzer": ["wappalyzer https://target.com"],
  "httprobe": ["cat hosts.txt | httprobe"],
  "gau": ["gau target.com"],

  "theHarvester": ["theHarvester -d target.com -b google"],
  "Maltego": ["Open Maltego GUI"],
  "SpiderFoot": ["spiderfoot -l 127.0.0.1:5001"],
  "Shodan": ["Use Shodan web UI"],
  "Censys": ["Use Censys web UI"],
  "Recon-ng": ["recon-ng"],

  "Nessus": ["Open Nessus web UI"],
  "OpenVAS": ["gvm-start"],
  "Nuclei": ["nuclei -u https://target.com -t cves/"],

  "Burp Suite": ["java -jar burpsuite.jar"],
  "OWASP ZAP": ["zap.sh"],
  "Nikto": ["nikto -h https://target.com"],
  "Sqlmap": ["sqlmap -u \"http://target/page.php?id=1\" --dbs"],
  "Dirsearch": ["python3 dirsearch.py -u https://target.com -e php,html"],
  "Gobuster": ["gobuster dir -u https://target.com -w wordlist.txt"],
  "FFUF": ["ffuf -u https://target/FUZZ -w wordlist.txt"],

  "Metasploit": ["msfconsole"],
  "Cobalt Strike": ["Open CS GUI"],
  "Sliver": ["sliver-server"],
  "Empire": ["./empire"],
  "CrackMapExec": ["cme smb target -u user -p pass"],
  "BloodHound": ["Start BH UI"],

  "Aircrack-ng": ["airmon-ng start wlan0", "aircrack-ng -w wordlist capture.cap"],
  "Reaver": ["reaver -i mon0 -b BSSID -vv"],
  "Kismet": ["kismet"],

  "Hashcat": ["hashcat -m 1000 -a 0 hashes.txt wordlist.txt"],
  "John the Ripper": ["john --wordlist=rockyou.txt hashes.txt"],
  "Hydra": ["hydra -l admin -P passwords.txt ssh://10.0.0.5"],

  "ScoutSuite": ["scoutsuite aws --report-dir ."],
  "Prowler": ["prowler"],
  "Pacu": ["pacu"],

  "MobSF": ["Start MobSF"],
  "Frida": ["frida -U -f com.app -l script.js --no-pause"],
  "apktool": ["apktool d app.apk"],

  "Ghidra": ["Launch Ghidra"],
  "Radare2": ["r2 -A binary"],
  "IDA": ["ida64 binary"],

  "Autopsy": ["autopsy"],
  "Volatility": ["volatility -f memdump.raw --profile=Win7SP1x64 pslist"],
  "Velociraptor": ["velociraptor -c config.yaml client"],

  "Cuckoo Sandbox": ["cuckoo"],
  "YARA": ["yara rules.yar sample.bin"],
  "Any.Run": ["Use Any.Run GUI"],

  "Wireshark": ["wireshark", "tshark -i eth0 -w capture.pcap"],
  "Tcpdump": ["tcpdump -i eth0 -w dump.pcap"],
  "Ettercap": ["ettercap -T -M arp:remote"],
  "Snort": ["snort -c /etc/snort/snort.conf -i eth0"],
  "Suricata": ["suricata -c /etc/suricata/suricata.yaml -i eth0"],

  "GRASSMARLIN": ["Run per README"],
  "Kali-ICS": ["Use Kali ICS toolkit"],

  "MISP": ["Start MISP"],
  "OpenCTI": ["Start OpenCTI"],

  "VSCode": ["code ."],
  "Docker": ["docker run -it image"]
}
